{
    "name": "mnapoli/php-di-symfony2",
    "type": "library",
    "description": "Integrates PHP-DI to Symfony 2",
    "homepage": "https://github.com/mnapoli/PHP-DI-Symfony2/",
    "license": "MIT",
    "autoload": {
        "psr-0": {
            "DI\\Bridge\\Symfony": "src/"
        }
    },
    "autoload-dev": {
        "psr-0": {
            "UnitTest\\DI\\Bridge\\Symfony": "tests/",
            "FunctionalTest\\DI\\Bridge\\Symfony": "tests/"
        }
    },
    "require": {
        "mnapoli/php-di": "~4.0",
        "symfony/dependency-injection": "~2.0"
    },
    "require-dev": {
        "symfony/config": "~2.0",
        "phpunit/phpunit": "~4.0"
    }
}
