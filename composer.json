{
    "name": "mnapoli/php-di-symfony2",
    "type": "library",
    "description": "Integrates PHP-DI to Symfony 2",
    "homepage": "https://github.com/mnapoli/PHP-DI-Symfony2/",
    "license": "MIT",
    "autoload": {
        "psr-0": {
            "DI\\Bridge\\Symfony\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-0": {
            "UnitTest\\DI\\Bridge\\Symfony\\": "tests/",
            "FunctionalTest\\DI\\Bridge\\Symfony\\": "tests/"
        }
    },
    "require": {
        "mnapoli/php-di": "~4.0|~5.0",
        "symfony/dependency-injection": "~2.3|~3.O",
        "symfony/http-kernel": "~2.3|~3.O",
        "symfony/proxy-manager-bridge": "~2.3|~3.O",
        "symfony/config": "~2.3|~3.O"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "symfony/filesystem": "~2.1|~3.O"
    }
}
